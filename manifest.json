{
"manifest_version": 2,
    "name": "Trump to Snake",
    "description": "Changes all instances of the word Trump to Snake",
    "version": "1.0",
    "icons": {
    	"128": "images/Trump-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}